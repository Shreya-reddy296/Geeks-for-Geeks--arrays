#include<iostream>
using namespace std;
void printArray(int a[], int n)
{
    for(int i=0;i<n;i++)
    {
        cout<<a[i]<<" ";
    }
}
void quickSort(int a[], int n)
{
    
}
int main()
{
    int a[] = {64, 34, 25, 12, 22, 11, 90};
    int n = sizeof(a)/sizeof(a[0]);
    quickSort(a, n);
    printf("Sorted array: \n");
    printArray(a, n);
}
